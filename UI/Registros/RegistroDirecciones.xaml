        <Label x:Name="label2" Content="Edificio/Piso/Apartamento" HorizontalAlignment="Left" Margin="65,412,0,0" VerticalAlignment="Top" Height="26" Width="149"/>
        <TextBox x:Name="textBox2" HorizontalAlignment="Left" Margin="225,419,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="265" Height="18"/>
        <Image x:Name="image4" HorizontalAlignment="Left" Height="21" Margin="27,470,0,0" VerticalAlignment="Top" Width="19" Source="/UI/Registros/calle.png"/>
