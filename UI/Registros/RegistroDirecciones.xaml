            </Button.Background>
        </Button>
        <Button x:Name="button1" Content="Guardar" HorizontalAlignment="Left" Margin="233,646,0,0" VerticalAlignment="Top" VerticalContentAlignment = "Bottom" Height="32" Width="47">
            <Button.Background>
                <ImageBrush ImageSource="/UI/Registros/icon_save.png" Stretch="Uniform" AlignmentY="Top" Viewport="0,0,1,0.5" />
            </Button.Background>
        </Button>
        <Button x:Name="button2" Content="Eliminar" HorizontalAlignment="Left" Margin="414,644,0,0" VerticalAlignment="Top" VerticalContentAlignment = "Bottom"  Height="34" Width="46">
            <Button.Background>
                <ImageBrush ImageSource="/UI/Registros/icon_trash.png" Stretch="Uniform" AlignmentY="Top" Viewport="0,0,1,0.5" />
