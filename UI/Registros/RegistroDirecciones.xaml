            <Image.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.712"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Image.RenderTransform>
        </Image>
        <Label x:Name="label" Content="DirecciónId" HorizontalAlignment="Left" Margin="55,293,0,0" VerticalAlignment="Top" Height="26" Width="71"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="143,297,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="18"/>
